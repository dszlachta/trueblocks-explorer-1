  "version": "0.1.0",
  "homepage": "./",
  "private": true,
  "scripts": {
    "compile:ui": "webpack --mode production",
    "develop:ui": "webpack serve --mode development --port 1234",
    "compile:electron": "tsc -p tsconfig_electron.json",
